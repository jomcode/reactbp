{
  "name": "reactbp",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "eslint": "eslint",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node src/server/index.js",
    "dev:server": "better-npm-run dev:server",
    "dev:client": "better-npm-run dev:client",
    "dev:client:hot": "better-npm-run dev:client:hot",
    "dev:hot": "better-npm-run dev:hot",
    "build:client": "better-npm-run build:client",
    "heroku-postbuild": "npm run build:client"
  },
  "betterScripts": {
    "dev:server": {
      "command": "nodemon src/server/index.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "dev:client": {
      "command": "webpack-dev-server",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "dev:client:hot": {
      "command": "webpack-dev-server",
      "env": {
        "NODE_ENV": "development",
        "HMR": "enabled"
      }
    },
    "dev:hot": {
      "command": "concurrent -k \"npm run dev:server\" \"npm run dev:client:hot\""
    },
    "build:client": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "keywords": [],
  "author": "John <jomcode@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "better-npm-run": "0.0.11",
    "clean-webpack-plugin": "^0.1.10",
    "concurrently": "^2.2.0",
    "css-loader": "^0.24.0",
    "dotenv": "^2.0.0",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.22.0",
    "postcss-loader": "^0.10.1",
    "react-hot-loader": "^3.0.0-beta.1",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.0",
    "webpack-merge": "^0.14.1"
  },
  "dependencies": {
    "core-js": "^2.4.1",
    "express": "^4.14.0",
    "normalize.css": "^4.2.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  }
}
